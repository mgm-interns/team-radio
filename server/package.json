{
  "name": "team-radio-server",
  "version": "1.0.0",
  "description": "Team radio server",
  "main": "dist/bundle.js",
  "scripts": {
    "predev": "npm run compile",
    "dev": "npm-run-all --parallel nodemon watch",
    "build": "npm run compile -- --env.NODE_ENV=production",
    "start": "node .",
    "nodemon": "nodemon",
    "compile": "webpack",
    "watch": "npm run compile -- --env.watch=enable",
    "clean": "rm -rf dist",
    "migrate:up": "node dist/migrate up",
    "migrate:down": "node dist/migrate down",
    "migrate:create": "migrate create --template-file .template/migration-template.ts",
    "postmigrate:create": "cp -r migrations/* src/migrations && rm -rf migrations"
  },
  "dependencies": {
    "bcrypt-nodejs": "0.0.3",
    "class-transformer": "^0.1.9",
    "class-validator": "^0.9.1",
    "graphql-yoga": "^1.16.1",
    "http-status-codes": "^1.3.0",
    "mongodb": "^3.1.3",
    "type-graphql": "^0.13.1",
    "typedi": "^0.8.0",
    "typeorm": "^0.2.7",
    "typeorm-typedi-extensions": "^0.2.1",
    "winston": "^3.0.0"
  },
  "devDependencies": {
    "@types/bcrypt-nodejs": "0.0.30",
    "@types/mongodb": "^3.1.4",
    "@types/winston": "^2.4.4",
    "dotenv": "^5.0.1",
    "dotenv-webpack": "^1.5.7",
    "migrate": "^1.6.0",
    "nodemon": "^1.18.3",
    "npm-run-all": "^4.1.3",
    "prettier": "^1.14.2",
    "prettier-loader": "^2.1.1",
    "ts-loader": "^4.5.0",
    "tslint": "^5.11.0",
    "tslint-config-airbnb": "^5.10.0",
    "tslint-config-prettier": "^1.15.0",
    "typescript": "^3.0.1",
    "webpack": "^4.17.1",
    "webpack-cli": "^3.1.0",
    "webpack-node-externals": "^1.7.2",
    "yargs": "^12.0.1"
  },
  "keywords": [
    "team-radio",
    "typescript",
    "webpack",
    "winston",
    "express",
    "graphql",
    "typedi",
    "typeorm",
    "type-graphql",
    "graphql",
    "mongodb",
    "migration"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mgm-interns/team-radio.git"
  },
  "author": "@pvtri96",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/mgm-interns/team-radio/issues"
  }
}
