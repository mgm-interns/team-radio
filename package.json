{
  "name": "team-radio",
  "version": "1.0.0-SNAPSHOT",
  "description": "A playlist for teams that can be edited collaboratively by all users.",
  "author": "@pvtri96",
  "license": "ISC",
  "scripts": {
    "start": "cd server && npm start",
    "install:shared": "cd shared && npm install && npm run compile",
    "install:server": "cd server && npm install",
    "install:client": "cd client && npm install",
    "install:admin": "cd admin && npm install",
    "build": "npm-run-all build:**",
    "build:server": "cd server && npm run build",
    "build:client": "cd client && npm run build",
    "build:admin": "cd admin && npm run build",
    "postinstall": "npm-run-all install:** build",
    "clean": "npm-run-all clean:**",
    "clean:node_modules:shared": "rimraf shared/node_modules",
    "clean:node_modules:server": "rimraf server/node_modules",
    "clean:node_modules:client": "rimraf client/node_modules",
    "clean:node_modules:admin": "rimraf admin/node_modules",
    "copy:env": "npm-run-all copy:env:**",
    "copy:env:server": "cd server && cp .env.example .env",
    "copy:env:client": "cd client && cp .env.example .env",
    "copy:env:admin": "cd admin && cp .env.example .env"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.3",
    "rimraf": "^2.6.3"
  },
  "homepage": "https://github.com/mgm-interns/team-radio#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mgm-interns/team-radio.git"
  },
  "bugs": {
    "url": "https://github.com/mgm-interns/team-radio/issues"
  },
  "engines": {
    "node": "10.x",
    "npm": "6.x"
  },
  "keywords": [
    "team-radio",
    "team-radio-next",
    "typescript",
    "webpack",
    "graphql",
    "typeorm",
    "graphql",
    "mongodb",
    "react",
    "react-admin",
    "redux",
    "websocket",
    "react-hooks"
  ]
}
