{
    "extends": [
      "tslint:latest",
      "tslint-react",
      "tslint-config-airbnb",
      "tslint-config-prettier"
    ],
    "rules": {
      "no-console": false,
      "no-namespace": false,
      //
      "variable-name": false,
      "no-empty-interface": false,
      "interface-name": false,
      "object-literal-sort-keys": false,
      "jsx-boolean-value": false,
      "jsx-no-lambda": false,
      "no-implicit-dependencies": false,
      "no-submodule-imports": false,
      "import-name": false,
      "function-name": false
    }
}
